#include<stdio.h>
void main()
{
    int i,n,pro=0,r;
    printf("Enter your Number:\n");
    scanf("%d",&n);
    for(i=n;i>0;i=i/10)
    {
        r=i%10;
        pro=pro+(r*r*r);
    }
    if(pro==n)
    printf("%d is an Armstrong Number",n);
    else
    printf("%d is not an Armstrong number",n);
}
